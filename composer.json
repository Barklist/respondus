{
  "name": "elsuterino/respondus",
  "description": "Typed resources.",
  "authors": [
    {
      "name": "Vytautas Riauka",
      "email": "wytuxas@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Elsuterino\\Respondus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2",
    "ext-json": "*"
  },
  "require-dev": {
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.2",
    "phpstan/phpstan": "^1.10"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "pestphp/pest-plugin": true
    },
    "preferred-install": {
      "*": "auto"
    },
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "lint": [
      "@php vendor/bin/phpstan analyse"
    ],
    "test": [
      "@php vendor/bin/pest"
    ]
  },
  "repositories": {
    "form": {
      "type": "path",
      "url": "../form"
    }
  }
}
